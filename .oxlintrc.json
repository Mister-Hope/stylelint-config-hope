{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "extends": ["./.oxlintrc.base.json"],
  "ignorePatterns": ["dist/"],
  "plugins": ["typescript", "oxc", "import", "jsdoc", "promise", "unicorn", "node"],
  "rules": {
    "jsdoc/check-tag-names": ["warn", { "definedTags": ["experimental"] }]
  }
}
